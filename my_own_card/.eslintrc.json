{
  "extends": [
    "eslint:recommended,",
    "react-app",
    "react-app/jest",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": ["prettier",
    "react",
    "@typescript-eslint",
    "import"],
  "rules": {
    "prettier/prettier":"error",
    "react/react-in-jsx-scope": "off"
  },
  
"settings": {
  "react": {
    "version": "detect"
  },
  "import/resolver": {
    "typescript": {
      "project": "./tsconfig.paths.json"
    }
  }
}
}

